  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Rekyl.Core\Rekyl.Core.csproj" />
  </ItemGroup>

</Project>
